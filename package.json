{
  "name": "sveltefire",
  "version": "0.2.0",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codediodeio/sveltefire.git"
  },
  "main": "dist/sveltefire.js",
  "module": "dist/sveltefire.es.js",
  "scripts": {
    "build": "vite build",
    "prepublishOnly": "npm run build",
    "serve": "vite",
    "e2e": "cypress open --config watchForFileChanges=true",
    "emulate": "cd emulate && firebase emulators:start --only firestore",
    "test:watch": "concurrently \"npm:serve\" \"npm:emulate\" \"npm:e2e\" ", 
    "update:build":"npm run update && npm prune && npm install && npm run build",
    "update":"npx --yes -p npm-check-updates ncu -u"
  },
  "files": [
    "README.md",
    "dist",
    "src"
  ],
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.39",
    "@testing-library/svelte": "^3.0.3",
    "concurrently": "^7.0.0",
    "cypress": "^9.5.1",
    "firebase-tools": "^10.2.2",
    "postcss": "^8.4.7",
    "postcss-load-config": "^3.1.3",
    "svelte": "^3.46.4",
    "svelte-hmr": "^0.14.9",
    "svelte-htm": "^1.1.1",
    "svelte-preprocess": "^4.10.4",
    "svelte-type-generator": "^1.1.0",
    "vite": "^2.8.6",
    "vite-web-test-runner-plugin": "^0.1.0",
    "firebase": "^9.6.7"
  },
  "peerDependencies": {
    "firebase": "^9.6.7"
  },
  "keywords": [
    "svelte",
    "firebase",
    "javascript"
  ],
  "types": "dist/types",
  "svelte": "src/index.js"
}
